{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "eslint-disable-next-line no-undef": "off",
    "react-hooks/exhaustive-deps": "warn",
    "react/prop-types": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ]
  }
}